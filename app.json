{
  "expo": {
    "name": "theknot",
    "slug": "theknot",
    "version": "1.7.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "theknot",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0178ff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dralsallum.theknot",
      "buildNumber": "54",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        },
        "NSPhotoLibraryUsageDescription": "This app requires access to your photo library to upload images.",
        "NSMicrophoneUsageDescription": "This app needs access to the microphone for audio playbook.",
        "NSContactsUsageDescription": "This app requires access to your contacts to add guests to your wedding.",
        "LSApplicationQueriesSchemes": ["about", "https", "http"],
        "SKAdNetworkItems": [
          {
            "SKAdNetworkIdentifier": "v9wttpbfk9.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "n38lu8286q.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "cj5566h2ga.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "n6fk4nfna4.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "cstr6suwn9.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "4fzdc2evr5.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "4pfyvq9l8r.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "2fnua5tdw4.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "3qy4746246.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "3sh42y64q3.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "f38h382jlk.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "6xzpu9s2p8.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "w9q455wk68.skadnetwork"
          },
          {
            "SKAdNetworkIdentifier": "yclnxrl5pm.skadnetwork"
          }
        ],
        "NSPrivacyPolicyURL": "https://www.treatment.net/privacy-policy",
        "LSMinimumSystemVersion": "12.0"
      },
      "notification": {
        "icon": "./assets/icons/notification.png",
        "color": "#0178ff",
        "badge": true
      }
    },
    "android": {
      "versionCode": 4,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "com.google.android.gms.permission.AD_ID",
        "android.permission.READ_CONTACTS"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.dralsallum.theknot",
      "privacyPolicyUrl": "https://www.treatment.net/privacy-policy",
      "termsOfServiceUrl": "https://www.treatment.net/terms-of-use"
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "react-native-iap",

      "react-native-video",
      [
        "expo-notifications",
        {
          "icon": "./assets/icons/notification.png",
          "color": "#0178ff"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2912eaf6-d462-44f2-80fc-e8767ae63b92"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"]
  }
}
